          "type": "validation",
          "bean": "nablarch.etl.integration.app.InputFile1Dto",
          "errorEntity": "nablarch.etl.integration.app.InputFile1ErrorEntity",
          "mode": "CONTINUE"
        },
        "load": {
          "type": "db2db",
          "bean": "nablarch.etl.integration.app.OutputTable1Entity",
          "sqlId": "SELECT_INPUT_FILE1"
        }
      }
    },
